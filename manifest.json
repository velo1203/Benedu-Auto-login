{
    "manifest_version": 3,
    "name": "BenEdu Auto-Login",
    "version": "1.0",
    "description": "Automatically logs into BenEdu platform.",
    "permissions": [
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "https://benedu.co.kr/"
    ],
    "content_scripts": [
        {
            "matches": ["https://benedu.co.kr/"],
            "js": ["content.js"]
        }
    ],
    "options_page": "options.html"
}
